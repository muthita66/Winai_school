model john_subject_sections {
  id          Int     @id @default(autoincrement())
  subject_id  Int?
  teacher_id  Int?
  year        Int?
  semester    Int?
  class_level String? @db.VarChar(10)
  classroom   String? @db.VarChar(10)
  day_of_week String? @db.VarChar(20)
  time_range  String? @db.VarChar(50)
  room        String? @db.VarChar(10)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model john_subjects {
  id            Int      @default(autoincrement())
  subject_code  String?  @db.VarChar(20)
  name          String   @db.VarChar(255)
  credit        Decimal? @db.Decimal
  name_th       String?  @db.VarChar(255)
  name_en       String?  @db.VarChar(255)
  subject_type  String?  @db.VarChar(50)
  subject_group String?  @db.VarChar(100)
  level         String?  @db.VarChar(50)
  total_hours   Int?
  description   String?
  year          Int?
  semester      Int?

  @@ignore
